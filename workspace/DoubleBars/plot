 "fort.9" using 6:8 lc rgb "#DC143C" notitle  with dots, \
"fort.9" using 10:12 lc rgb "#006400" notitle  with dots, \
 "fort.9" using 14:16 lc rgb "#FF00FF" notitle  with dots

